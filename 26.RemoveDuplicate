var removeDuplicates = function (nums) {

    let map = new Map()
    let result = []
    for (let i = 0; i < nums.length; i++) {
        if (map.has(nums[i])) {
            map.set(nums[i], map.get(nums[i]) + 1)
        } else {
            map.set(nums[i], 1)
            result.push(nums[i])
            
        }
    }
    // console.log(result)
    for(let i  =0 ; i < nums.length ; i++){
        nums[i] = result[i]
    }
    nums.splice(result.length)

};